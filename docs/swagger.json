                            "address": {
                              "type": "string",
                              "example": "Samarinda"
                            },
                            "carsId": {
                              "type": "array",
                              "example": []
                            },
                            "updatedAt": {
                              "type": "string",
                              "example": "2023-10-21T06:45:45.599Z"
                            },
                            "createdAt": {
                              "type": "string",
                              "example": "2023-10-21T06:45:45.599Z"
                            }
                          }
                        },
                        "_previousDataValues": {
                          "type": "object",
                          "properties": {
                            "id": {
                              "type": "integer",
                              "example": 6
                            },
                            "name": {
                              "type": "string",
                              "example": "arsel"
                            },
                            "age": {
                              "type": "integer",
                              "example": 18
                            },
                            "role": {
                              "type": "string",
                              "example": "Member"
                            },
                            "address": {
                              "type": "string",
                              "example": "Samarinda"
                            },
                            "carsId": {
                              "type": "array",
                              "example": []
                            },
                            "updatedAt": {
                              "type": "string",
                              "example": "2023-10-21T06:45:45.599Z"
                            },
                            "createdAt": {
                              "type": "string",
                              "example": "2023-10-21T06:45:45.599Z"
                            }
                          }
                        },
                        "uniqno": {
                          "type": "integer",
                          "example": 1
                        },
                        "_changed": {
                          "type": "object",
                          "example": {}
                        },
                        "_options": {
                          "type": "object",
                          "properties": {
                            "isNewRecord": {
                              "type": "boolean",
                              "example": true
                            },
                            "_schema": {
                              "type": "string",
                              "example": null
