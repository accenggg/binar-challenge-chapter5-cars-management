                            "_schemaDelimiter": {
                              "type": "string",
                              "example": ""
                            }
                          }
                        },
                        "isNewRecord": {
                          "type": "boolean",
                          "example": false
                        },
                        "email": {
                          "type": "string",
                          "example": "arsel@gmail.com"
                        },
